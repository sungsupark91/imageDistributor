{
    "targetExtensions": ["JPG", "JPEG", "PNG", "HEIC", "ARW", "RAF", "DNG", "MP4", "MOV"],
    "pathRules": [
        {
            "from": "/volume1/photo_raw/이미지자동분류(raw)",
            "to": "/volume1/photo_raw",
            "folderFormat": "@년/@년-@월-@일",
            "fileFormat": "@년@월@일@시@분@초 @모델명"
        },
        {
            "from": "/volume1/photo/이미지자동분류(수빈)",
            "to": "/volume1/photo/수빈",
            "folderFormat": "@년/@월@일",
            "fileFormat": "@년@월@일@시@분@초 @모델명"
        },
        {
            "from": "/volume1/photo/이미지자동분류(개인)",
            "to": "/volume1/photo/일반스냅",
            "folderFormat": "@년/@년-@월",
            "fileFormat": "@년@월@일@시@분@초 @모델명"
        }
        
    ],
    "flags": {
        "subFolderByPhone": false,
        "subFolderByRaw": false,
        "subFolderByVideo": true,
        "remainFilmInfo": false,
        "folderWithDescription": true,
        "usePolling": false
    }
}
